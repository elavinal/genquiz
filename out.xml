  <question type="multichoice">
    <name>
      <text>parameter-passing</text>
    </name>
    <defaultgrade>1</defaultgrade>
    <single>false</single>
    <questiontext format="html">
      <text><![CDATA[<p><p>Consider the following function:</p></p>
<img alt="code-fig" src="data:image/png;base64,">
<p><p>After the line <code>int n = 2, *p;</code>, how is it possible to modify the value of <code>n</code>?</p></p>]]></text>
    </questiontext>
    <shuffleanswers>1</shuffleanswers>
    <answernumbering>none</answernumbering>
    <answer fraction="50" format="html">
      <text><![CDATA[<code>add_ten(&n);</code>]]></text>
    </answer>
    <answer fraction="50" format="html">
      <text><![CDATA[<code>p = &n; add_ten(p);</code>]]></text>
    </answer>
    <answer fraction="-50" format="html">
      <text><![CDATA[<code>add_ten(n);</code>]]></text>
    </answer>
    <answer fraction="-50" format="html">
      <text><![CDATA[<code>add_ten(p);</code>]]></text>
    </answer>
    <answer fraction="-50" format="html">
      <text><![CDATA[<code>*p = n; add_ten(p);</code>]]></text>
    </answer>
